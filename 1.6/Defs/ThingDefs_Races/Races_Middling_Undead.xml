<Defs>

    <!-- Ghoul -->  
    <ThingDef ParentName="BMT_BaseUndead">
      <defName>BMT_Ghoul</defName>
      <label>ghoul</label>
      <description>Despite their emaciated and decaying body, the ghoul remains a terrifying undead creature thanks to their razor-like claws and jagged teeth. Their nasty claws tend to cause infections in the wounds of living creatures, should they even survive a mauling.</description>
      <statBases>
        <MoveSpeed>4.35</MoveSpeed>
		<ArmorRating_Blunt>0.10</ArmorRating_Blunt>
		<ArmorRating_Sharp>0.05</ArmorRating_Sharp>	
      </statBases>
	  	<tools>
	  <li>
		<label>Claws</label>
		<capacities>
          <li>Cut</li>
          <li>Stab</li>
		</capacities>
		<power>2</power>
		<cooldownTime>2</cooldownTime>
		<linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
		<extraMeleeDamages>
		  <li>
			<def>Stun</def>
			<amount>8</amount>
			<chance>0.5</chance>
		  </li>
		  <li>
			<def>BMT_NoxiousCut</def>
			<amount>8</amount>
			<chance>0.5</chance>
		  </li>
		  <li>
			<def>BMT_NoxiousStab</def>
			<amount>8</amount>
			<chance>0.25</chance>
		  </li>
		</extraMeleeDamages>
	  </li>
	  <li>
		<label>Claws</label>
		<capacities>
          <li>Cut</li>
          <li>Stab</li>
		</capacities>
		<power>2</power>
		<cooldownTime>2</cooldownTime>
		<linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
		<extraMeleeDamages>
		  <li>
			<def>Stun</def>
			<amount>8</amount>
			<chance>0.5</chance>
		  </li>
		  <li>
			<def>BMT_NoxiousCut</def>
			<amount>8</amount>
			<chance>0.5</chance>
		  </li>
		  <li>
			<def>BMT_NoxiousStab</def>
			<amount>8</amount>
			<chance>0.25</chance>
		  </li>
		</extraMeleeDamages>
	  </li>
	  <li>
		<label>Teeth</label>
		<capacities>
          <li>Stab</li>
		</capacities>
		<power>7</power>
		<cooldownTime>2</cooldownTime>
		<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
		<extraMeleeDamages>
		  <li>
			<def>Stun</def>
			<amount>8</amount>
			<chance>0.5</chance>
		  </li>
		</extraMeleeDamages>
	  </li>
	</tools>
      <race>
		<isImmuneToInfections>true</isImmuneToInfections>
		<bleedRateFactor>0.0</bleedRateFactor>
		<mechWeightClass>Medium</mechWeightClass>
      </race>
    </ThingDef>
    
