	<!-- Undying Disciple -->  
    <ThingDef ParentName="BMT_BaseUndead">
      <defName>BMT_UndyingDisciple</defName>
      <label>undying disciple</label>
      <description>A more intelligent skeleton capable of doing skilled magical attacks. It has a medium-durability arcane shield around it.</description>
      <statBases>
        <MoveSpeed>4</MoveSpeed>
		<ArmorRating_Blunt>0.10</ArmorRating_Blunt>
		<ArmorRating_Sharp>0.20</ArmorRating_Sharp>	
      </statBases>
      <race>
		<isImmuneToInfections>true</isImmuneToInfections>
		<bleedRateFactor>0.0</bleedRateFactor>
		<mechWeightClass>Medium</mechWeightClass>
      </race>
    <comps>
      <li Class="CompProperties_ProjectileInterceptor">
        <radius>3</radius>
        <interceptGroundProjectiles>true</interceptGroundProjectiles>
        <color>(0.4, 0.4, 0.4)</color>
        <reactivateEffect>BulletShieldGenerator_Reactivate</reactivateEffect>
        <activeSound>BulletShield_Ambience</activeSound>
        <hitPoints>200</hitPoints>
        <hitPointsRestoreInstantlyAfterCharge>true</hitPointsRestoreInstantlyAfterCharge>
        <chargeDurationTicks>5400</chargeDurationTicks><!-- 90s -->
        <drawWithNoSelection>True</drawWithNoSelection>
        <gizmoTipKey>ProjectileInterceptorTip</gizmoTipKey>
      </li>
    </comps>
    </ThingDef>
    
